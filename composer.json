{
    "name": "secureheaders/psradapter",
    "description": "SecureHeaders Psr7 Http Adapter",
    "keywords": [
        "secureheaders",
        "psr7"
    ],
    "type": "library",
    "require": {
        "php": ">=7.0",
        "aidantwoods/secureheaders": "^2.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5||^8.0",
        "zendframework/zend-diactoros": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SecureHeaders\\PsrHttpAdapter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SecureHeaders\\PsrHttpAdapter\\Tests\\": "tests/"
        }
    },
    "license": "MIT"
}
